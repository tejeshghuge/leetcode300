class Solution {
    public ListNode deleteMiddle(ListNode head) {
         if(head == null || head.next == null) return null;
                    ListNode slow= head,fast = head ,dummy = head;
                  
                    while(fast != null && fast.next != null){

            slow = slow.next;
            fast = fast.next.next;

        }
        while(dummy.next != slow){
            dummy = dummy.next;
      
    }
    dummy.next = dummy.next.next;
    return head;
}
}
